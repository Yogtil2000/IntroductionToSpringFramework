<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean class="in.TodayLearning.beans.Student" id="stdId1">
    
        <property name="name" value="Deepak"></property>
        <property name="age" value="30"></property>
        <property name="email" value="deepak@gmail.com"></property>
        
    </bean>
    
     <bean class="in.TodayLearning.beans.Student" id="stdId2">
    
        <property name="name" value="Amit"></property>
        <property name="age" value="33"></property>
        <property name="email" value="amit@gmail.com"></property>
        
    </bean>

</beans>